<%= form_with model: @booking, url: booking_path(@booking), method: :patch do |form| %>
  <%= form.label :start_time %>
  <%= form.datetime_select :start_time %>

  <%= form.label :appointment %>
  <%= form.collection_select :appointment_id, Appointment.all, :id, :title %>

  <%= form.label :user %>
  <%= form.collection_select :user_id, User.all, :id, :email %>

  <%= form.submit %>
<% end %>
